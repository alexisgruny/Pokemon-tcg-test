{
    "name": "pokemontcgptrade",
    "version": "1.0.0",
    "description": "Un mod√®le de projet TypeScript avec Node.js",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc && npm run copy-assets",
        "start": "node dist/index.js",
        "dev": "nodemon --watch src --exec ts-node src/index.ts",
        "ts:watch": "tsc --watch",
        "watch:build": "nodemon --watch src --ext ts,ejs,css,js --exec \"npm run build\"",
        "copy-assets": "npm run copy-views && npm run copy-public",
        "copy-views": "xcopy src\\views dist\\views /E /I /Y",
        "copy-public": "xcopy src\\public dist\\public /E /I /Y"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/express-session": "^1.18.1",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/node": "^20.0.0",
        "@types/passport": "^1.0.17",
        "@types/passport-google-oauth20": "^2.0.16",
        "nodemon": "^2.0.0",
        "ts-node": "^10.0.0",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "@types/express": "^4.17.21",
        "axios": "^1.8.4",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "ejs": "^3.1.10",
        "express": "^5.1.0",
        "express-session": "^1.18.1",
        "google-auth-library": "^9.15.1",
        "jsonwebtoken": "^9.0.2",
        "node-cron": "^3.0.3",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "pg": "^8.14.1",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.37.7"
    }
}
